{
  "compilerOptions": {
    "module": "<% module %>",
    "moduleResolution": "node",
    "target": "<% es %>",
    "emitBOM": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "pretty": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.iterable",
      "es2015.promise"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./<% structureConfig.src %>/<% structureConfig.external %>/<% structureConfig.types %>"
    ],
    "types": [
      "@cdp/mobile",
      "patch.dependencies",
      "requirejs",
      <%# cordova %>
      "cordova",
      <%/ cordova %>
      "jasmine"
    ]
  }
}
